#include <stdio.h>

int main() {
    int arr[] = {12, 45, 6, 89, 23, 7, 56};
    int n = sizeof(arr) / sizeof(arr[0]); 
    int max, min;
    int i;
    max = arr[0];
    min = arr[0];
    for (i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
        if (arr[i] < min) {
            min = arr[i];
        }
    }

    printf("Maximum element in the array: %d\n", max);
    printf("Minimum element in the array: %d\n", min);

    return 0;
}